{
  "name": "sb-element",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:sb": "npm run build --prefix ./src/sb-element && npm run build --prefix ./src/schematics",
    "dev:sb": "npm run dev --prefix ./src/sb-element",
    "build:scripts": "tsc -p ./scripts/tsconfig.scripts.json",
    "dev:scripts": "tsc -p ./scripts/tsconfig.scripts.json --watch",
    "build:doc": "node ./dist/scripts/sb-element-docs.js",
    "dev:doc": "npx nodemon --config ./nodemon.parse-doc.json ./dist/scripts/sb-element-docs.js",
    "build:schematics": "npm run build --prefix ./src/schematics",
    "dev:schematics": "npm run dev --prefix ./src/schematics",
    "test:sb": "ng test sb-element"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "date-fns": "^2.24.0",
    "highlight.js": "^11.5.1",
    "ng-packager": "^1.0.0",
    "rxjs": "~6.6.0",
    "sb-element": "file:dist/sb-element",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-devkit/schematics": "^13.3.6",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@schematics/angular": "^13.3.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^17.0.35",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "nodemon": "^2.0.16",
    "sass-loader": "^12.6.0",
    "tree-model": "^1.0.7",
    "typedoc": "^0.22.15",
    "typescript": "^4.4.4"
  }
}