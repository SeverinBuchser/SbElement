{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "0f6cfd45-a9ae-4322-a71f-2cf83aa4761e"
  },
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "example-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "example-app",
      "sourceRoot": "src/example-app",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/example-app",
            "index": "src/example-app/index.html",
            "main": "src/example-app/main.ts",
            "polyfills": "src/example-app/polyfills.ts",
            "tsConfig": "src/example-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/example-app/favicon.ico",
              "src/example-app/assets"
            ],
            "styles": [
              "src/example-app/styles.scss",
              {
                "input": "src/example-app/sb-dark.scss",
                "inject": false,
                "bundleName": "sb-dark"
              },
              {
                "input": "src/example-app/sb-neutral.scss",
                "inject": false,
                "bundleName": "sb-neutral"
              },
              {
                "input": "src/example-app/sb-light.scss",
                "inject": false,
                "bundleName": "sb-light"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/example-app"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/example-app/environments/environment.ts",
                  "with": "src/example-app/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "example-app:build:production"
            },
            "development": {
              "browserTarget": "example-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "example-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/example-app/test.ts",
            "polyfills": "src/example-app/polyfills.ts",
            "tsConfig": "src/example-app/tsconfig.spec.json",
            "karmaConfig": "src/example-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/example-app/favicon.ico",
              "src/example-app/assets"
            ],
            "styles": [
              "src/example-app/styles.scss",
              {
                "input": "src/example-app/sb-dark.scss",
                "inject": false,
                "bundleName": "sb-dark"
              },
              {
                "input": "src/example-app/sb-neutral.scss",
                "inject": false,
                "bundleName": "sb-neutral"
              },
              {
                "input": "src/example-app/sb-light.scss",
                "inject": false,
                "bundleName": "sb-light"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/example-app"
              ]
            },
            "scripts": []
          }
        }
      }
    },
    "sb-element": {
      "projectType": "library",
      "root": "sb-element",
      "sourceRoot": "src/sb-element",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "src/sb-element/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "src/sb-element/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "src/sb-element/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/sb-element/test.ts",
            "tsConfig": "src/sb-element/tsconfig.spec.json",
            "karmaConfig": "src/sb-element/karma.conf.js",
            "codeCoverage": true,
            "codeCoverageExclude": [
              "**/testing/**/*"
            ]
          }
        }
      }
    }
  }
}
