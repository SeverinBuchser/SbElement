{
  "name": "sb-element-schematics",
  "scripts": {
    "build": "rm -rf ../../dist/sb-element/schematics && tsc --project tsconfig.schematics.json",
    "postbuild": "copyfiles ./*/schema.json ./*/files/*/** ./collection.json ../../dist/sb-element/schematics",
    "dev": "npx nodemon --config ./nodemon.schematics.json --exec npm run build"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript",
    "nodemon": "file:../../node_modules/nodemon"
  }
}
