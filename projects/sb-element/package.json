{
  "name": "sb-element",
  "version": "1.0.12",
  "author": "Severin Buchser",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeverinBuchser/SbElement.git"
  },
  "scripts": {
    "build": "ng build SbElement --configuration production && tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/sb-element/",
    "dev": "ng build SbElement --watch",
    "doc": "compodoc --config .compodocrc",
    "copy:doc": "cp -ra ./documentation/. ../../../SeverinBuchser.github.io/sb-element-doc"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "peerDependencies": {
    "@angular/common": "~13.2.0",
    "@angular/core": "~13.2.0",
    "date-fns": "^2.24.0"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "keywords": [
    "library",
    "angular",
    "sb",
    "component",
    "sb-theming",
    "sb-element"
  ],
  "exports": {
    ".": {
      "sass": "./src/style/_index.scss"
    },
    "./global": {
      "sass": "./src/style/global/_index.scss"
    },
    "./core": {
      "sass": "./src/style/core/_index.scss"
    },
    "./core/animation": {
      "sass": "./src/style/core/animation/_index.scss"
    },
    "./core/border": {
      "sass": "./src/style/core/border/_index.scss"
    },
    "./core/box-shadow": {
      "sass": "./src/style/core/box-shadow/_index.scss"
    },
    "./core/display": {
      "sass": "./src/style/core/display/_index.scss"
    },
    "./core/spacing": {
      "sass": "./src/style/core/spacing/_index.scss"
    },
    "./core/theming": {
      "sass": "./src/style/core/theming/_index.scss"
    },
    "./core/transitioning": {
      "sass": "./src/style/core/transitioning/_index.scss"
    },
    "./core/typography": {
      "sass": "./src/style/core/typography/_index.scss"
    },
    "./core/util": {
      "sass": "./src/style/core/util/_index.scss"
    }
  }
}
