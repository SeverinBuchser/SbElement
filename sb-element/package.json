{
  "name": "sb-element",
  "version": "1.0.13",
  "author": "Severin Buchser",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeverinBuchser/SbElement.git"
  },
  "scripts": {
    "build": "ng build SbElement --configuration production && tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../dist/sb-element/",
    "dev": "ng build SbElement --watch",
    "doc": "compodoc --config .compodocrc",
    "copy:doc": "cp -ra ./documentation/. ../../SeverinBuchser.github.io/sb-element-doc"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "peerDependencies": {
    "@angular/common": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/cdk": "^13.3.4",
    "date-fns": "^2.24.0",
    "highlight.js": "^11.5.1"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "copyfiles": "file:../node_modules/copyfiles",
    "typescript": "file:../node_modules/typescript"
  },
  "keywords": [
    "library",
    "angular",
    "sb",
    "component",
    "sb-theming",
    "sb-element"
  ],
  "exports": {
    ".": {
      "sass": "./src/lib/sb-element.scss"
    },
    "./theme": {
      "sass": "./src/lib/sb-element.theme.scss"
    },
    "./theme/dark": {
      "sass": "./src/lib/sb-element.theme.dark.scss"
    },
    "./theme/light": {
      "sass": "./src/lib/sb-element.theme.light.scss"
    },
    "./core": {
      "sass": "./src/lib/core/_module.scss"
    },
    "./core/animation": {
      "sass": "./src/lib/core/animation/_index.scss"
    },
    "./core/border": {
      "sass": "./src/lib/core/border/_index.scss"
    },
    "./core/box-shadow": {
      "sass": "./src/lib/core/box-shadow/_index.scss"
    },
    "./core/display": {
      "sass": "./src/lib/core/display/_index.scss"
    },
    "./core/spacing": {
      "sass": "./src/lib/core/spacing/_index.scss"
    },
    "./core/theming": {
      "sass": "./src/lib/core/theming/_index.scss"
    },
    "./core/transitioning": {
      "sass": "./src/lib/core/transitioning/_index.scss"
    },
    "./core/typography": {
      "sass": "./src/lib/core/typography/_index.scss"
    },
    "./core/util": {
      "sass": "./src/lib/core/util/_index.scss"
    }
  }
}
